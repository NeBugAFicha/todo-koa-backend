{
  "name": "koa_project",
  "version": "1.0.0",
  "description": "test_backend",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development node dist/index.js",
    "build": "rimraf ./dist && tsc",
    "test": "npm run resetdb && npm run build && jest",
    "migrate": "prisma migrate dev --name init",
    "resetdb": "prisma migrate reset --force",
    "lint": "eslint ./app",
    "format": "eslint ./app --fix",
    "prettier": "prettier --write ./app",
    "lint:prettier": "npm run lint && npm run format && npm run prettier"
  },
  "author": "me",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@prisma/client": "^4.8.0",
    "@types/koa": "^2.13.5",
    "@types/koa-router": "^7.4.4",
    "bcrypt": "^5.1.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^12.0.0",
    "prisma": "^4.8.0",
    "rimraf": "^4.0.4",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "supertest": "^6.3.3",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.2"
  }
}
